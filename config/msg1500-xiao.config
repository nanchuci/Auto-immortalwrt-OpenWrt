#
# 机型选择
#
CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_raisecom_msg1500-x-00=y

#
# MTK驱动选项
#
CONFIG_FIRST_IF_EEPROM_FLASH=y
CONFIG_MTK_FIRST_IF_EPAELNA=y
# CONFIG_MTK_FIRST_IF_IPAILNA is not set
CONFIG_MTK_INTERWORKING=y
CONFIG_MTK_MBO_SUPPORT=y
# CONFIG_MTK_SECOND_IF_MT7615E is not set
CONFIG_MTK_SECOND_IF_NONE=y
CONFIG_MULTI_PROFILE_SUPPORT=y
CONFIG_MTK_LED_CONTROL_SUPPORT=y
CONFIG_SINGLE_SKU=y
CONFIG_HW_NAT=y
CONFIG_FAST_NAT_SUPPORT=y
CONFIG_RA_NAT_HW=y
CONFIG_RA_HW_NAT=m
CONFIG_RA_HW_NAT_LAN_VLANID=1
CONFIG_RA_HW_NAT_WAN_VLANID=2
# CONFIG_RA_HW_NAT_TBL_1K is not set
# CONFIG_RA_HW_NAT_TBL_2K is not set
CONFIG_RA_HW_NAT_TBL_4K=y
# CONFIG_RA_HW_NAT_TBL_8K is not set
# CONFIG_RA_HW_NAT_TBL_16K is not set
CONFIG_HNAT_V2=y
CONFIG_RA_HW_NAT_IPV6=y
CONFIG_RA_HW_NAT_WIFI=y
CONFIG_RA_HW_NAT_WIFI_NEW_ARCH=y
CONFIG_SUPPORT_WLAN_OPTIMIZE=y
CONFIG_SUPPORT_WLAN_OPTIMIZE_TX=y
CONFIG_SUPPORT_WLAN_OPTIMIZE_RX=y
#
#开启off加速
#
CONFIG_PACKAGE_kmod-ipt-offload=y
CONFIG_PACKAGE_kmod-nf-flow=y
#CONFIG_PACKAGE_TURBOACC_INCLUDE_SHORTCUT_FE=y
#CONFIG_PACKAGE_kmod-fast-classifier=y
#CONFIG_PACKAGE_fast-classifier=y
CONFIG_PACKAGE_shortcut-fe=y
#CONFIG_PACKAGE_shortcut-fe-simulated-driver=y
#CONFIG_PACKAGE_kmod-shortcut-fe=y
#CONFIG_PACKAGE_kmod-shortcut-fe-drv=y
#CONFIG_PACKAGE_kmod-shortcut-fe-cm=y
CONFIG_PACKAGE_luci-app-turboacc=y
#CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_shortcut-fe=y
#CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_BBR_CCA=y
#CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_DNSFORWARDER=y
#CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_DNSPROXY=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_OFFLOADING=y
#CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_SHORTCUT_FE_CM=y
#CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_SHORTCUT_FE_DRV=y

#
# IPV6支持
#
CONFIG_IPV6=y
CONFIG_PACKAGE_dnsmasq_full_dhcpv6=y
CONFIG_PACKAGE_ipv6helper=y
CONFIG_PACKAGE_odhcp6c=y
CONFIG_PACKAGE_odhcpd-ipv6only=y
CONFIG_PACKAGE_luci-proto-ipv6=y
CONFIG_PACKAGE_ip6tables=y
CONFIG_PACKAGE_ip6tables-extra=y
CONFIG_PACKAGE_ip6tables-mod-nat=y
CONFIG_PACKAGE_kmod-ipt-nat6=y
CONFIG_PACKAGE_kmod-ip6tables=y
CONFIG_PACKAGE_kmod-ip6tables-extra=y
CONFIG_PACKAGE_iputils-traceroute6=y
CONFIG_PACKAGE_6in4=y
CONFIG_PACKAGE_6rd=y
CONFIG_PACKAGE_6to4=y
CONFIG_PACKAGE_luci-app-nat6-helper=y

#
# USB与存储相关支持
#
CONFIG_PACKAGE_automount=y
CONFIG_PACKAGE_ntfs-3g=y
#CONFIG_PACKAGE_kmod-mtd-rw=y
CONFIG_PACKAGE_kmod-usb-printer=y

#
# 插件集成
#
CONFIG_PACKAGE_adbyby=y
CONFIG_PACKAGE_fullconenat=y
CONFIG_PACKAGE_kmod-ramips_hnat=y
#CONFIG_PACKAGE_natflow=y
#CONFIG_PACKAGE_kmod-natflow=y
#CONFIG_PACKAGE_natflow-boot=y
#CONFIG_PACKAGE_natflow-auth=y
#CONFIG_PACKAGE_luci-app-natflow-users=y
CONFIG_PACKAGE_mtk_apcli=y
CONFIG_PACKAGE_mtkiappd=y
CONFIG_PACKAGE_luci-app-adbyby-plus=y
CONFIG_PACKAGE_luci-app-dnsfilter=y
CONFIG_PACKAGE_luci-app-flowoffload=y
#CONFIG_PACKAGE_luci-app-store=y
CONFIG_PACKAGE_luci-app-eqos=y
CONFIG_PACKAGE_luci-app-usb-printer=y

#多播
CONFIG_PACKAGE_luci-app-syncdial=y
CONFIG_PACKAGE_luci-app-mwan3=y
CONFIG_PACKAGE_luci-app-mwan3helper=y

#IPTV组播
#CONFIG_PACKAGE_luci-app-udpxy=y
#CONFIG_PACKAGE_luci-app-iptvhelper=y

#
# 主题
#
CONFIG_PACKAGE_luci-theme-argon-18.06=y
